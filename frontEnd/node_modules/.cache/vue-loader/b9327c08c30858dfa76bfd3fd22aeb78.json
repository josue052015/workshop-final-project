\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      elements: [],\r\n      card: true,\r\n    };\r\n  },\r\n  methods: {\r\n    changeCardState() {\r\n      this.card =! this.card;\r\n    },\r\n  },\r\n  async created() {\r\n    const response = await axios.get(\"https://localhost:44394/api/workshop\");\r\n    this.elements = response.data.slice(this.elements.length - 3);\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.center {\r\n  text-align: center;\r\n  padding: 8px;\r\n}\r\n</style>"]}]}